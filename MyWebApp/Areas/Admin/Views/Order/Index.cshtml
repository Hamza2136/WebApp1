@{
    ViewData["Title"] = "Index";
}

<div class="container">
    <div class="row">
        <div class="col-6 text-primary p-4">
            <h2>List of Orders</h2>
        </div>
        <div class="col-6 text-end p-4">
            <a class="btn btn-primary" asp-area="admin" asp-action="AddUpdate" asp-controller="product"><i class="bi bi-plus-square"></i>&nbspClick to Add Category</a>
        </div>
    </div>
</div>
<table id="mytable" class="table table-striped table-bordered w-100">
    <thead>
        <tr>
            <th>Name</th>
            <th>Phone Number</th>
            <th>Order Status</th>
            <th>Order Total</th>
            <th>Action</th>
        </tr>
    </thead>
</table>

@section scripts{
    <script src="~/js/AllOrder.js"></script>
}

